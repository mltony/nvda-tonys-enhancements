msgstr "Copiar &columna"

msgid "Copy column from current cell &down"
msgstr "Copiar columna desde la celda actual hacia &abajo"

msgid "Copy &row"
msgstr "Copiar &fila"

msgid "Copy row from current cell to the ri&ght"
msgstr "Copiar fila desde la celda actual a la &derecha"

msgid "Copy &table"
msgstr "Copiar &tabla"

