msgstr "Fila parcial copiada"

msgid "Column copied"
msgstr "Columna copiada"

msgid "Partial column copied"
msgstr "Columna parcial copiada"

msgid "Table copied"
msgstr "Tabla copiada"

msgid "Copy ce&ll"
msgstr "Copiar c&elda"

