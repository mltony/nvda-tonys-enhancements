#. when the cursor is not within a table.
msgid "Not in a table cell"
msgstr "No estás en una celda de tabla"

msgid "Cannot find edge of table in this direction"
msgstr "No se puede encontrar el borde de la tabla en esta dirección"

#, python-brace-format
msgid "Cannot find {axis} with index {index} in this table"
msgstr "No se puede encontrar {axis} con índice {index} en esta tabla"

msgid "Not in a table!"
msgstr "¡No estás en una tabla!"

